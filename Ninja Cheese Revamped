while true do
	wait(0.1)

	local niggas = game.Players:GetPlayers()

	print("iwantkfc")
	
	for _, player in pairs(niggas) do
		
		if player and player.Character then
			local nigga = player.Character


			local niggasstillhere = nigga:FindFirstChildOfClass("Humanoid")

			if niggasstillhere then
				print("nigga")
				player:Kick("niggacheeseisntcheesing")
			end
		end
	end
end
